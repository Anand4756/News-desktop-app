<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
  <div class="navbar">
<div class="logo">
  <h2>Dnews</h2>
</div>
  </div>
  <hr>
<div class="nav-category">
<ul>
  <a href=""><li>Sports</li></a>
  <a href=""><li>World</li></a>
  <a href=""><li>Economy</li></a>
  <a href=""><li>Entertainment</li></a>
  <a href=""><li>Trending</li></a>
  <a href=""><li>Technology</li></a>
</ul>
</div>
<section  class="latest-news">
<div class="heading">
  <h2>Latest Updates</h2>
</div>

<div class="news-div">
  
       
<div class="news-box1">
   
</div>
<div class="news-box2">
  
  
</div>
</div>

</section>

<div class="row-2">
  <% articles.forEach(function(article){ %>
    <% if ((typeof article.url=='object') || (typeof article.title=='object') || (typeof article.urlToImage=='object') || (typeof article.content=='object')){ %>
      <% } else{ %>
        <a href="<%= article.url %>" target="_blank" class="news-box Hover-effect">
          <img src="<%= article.urlToImage %>" alt="Image">
          <h3>
              <%= article.title %>
          </h3>
          <p>
              <%= article.content %>
          </p>
          <br>
          <% } %>
      <% }) %>
          </div>
  

  </body>
</html>
